{
  "name": "simplicate-source",
  "version": "0.1.76",
  "description": "Simplicate Airbyte source",
  "keywords": [
    "airbyte",
    "source",
    "simplicate"
  ],
  "homepage": "https://www.faros.ai",
  "author": "Faros AI, Inc.",
  "license": "Apache-2.0",
  "files": [
    "lib/",
    "resources/"
  ],
  "engines": {
    "node": ">=14.5"
  },
  "main": "./lib",
  "scripts": {
    "build": "tsc -p src",
    "clean": "rm -rf lib node_modules out",
    "fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts' && npm run lint -- --fix",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "prepare": "yarn build",
    "test": "jest --verbose --color",
    "test-cov": "jest --coverage --verbose --color",
    "watch": "tsc -b -w src test"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "commander": "^9.0.0",
    "faros-airbyte-cdk": "^0.1.76",
    "verror": "^1.10.1"
  },
  "jest": {
    "coverageDirectory": "out/coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".d.ts",
      ".js"
    ],
    "testTimeout": 10000,
    "globals": {
      "ts-jest": {
        "tsconfig": "test/tsconfig.json"
      }
    }
  },
  "devDependencies": {
    "@types/analytics-node": "^3.1.8",
    "@types/axios": "^0.14.0",
    "@types/eslint": "^8.4.2",
    "@types/fast-redact": "^3.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jenkins": "^0.23.2",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.173",
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.31",
    "@types/parse-link-header": "2.0.0",
    "@types/pino": "^7.0.5",
    "@types/temp": "^0.9.1",
    "@types/toposort": "^2.0.3",
    "@types/traverse": "^0.6.32",
    "@types/turndown": "^5.0.1",
    "@types/uuid": "^8.3.4",
    "@types/verror": "^1.10.5",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-extended": "^1.2.0",
    "lerna": "^4.0.0",
    "lint-staged": "^12.4.1",
    "mockttp": "^3.0.0",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.6.2",
    "temp": "^0.9.4",
    "ts-essentials": "^9.1.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.4"
  }
}
